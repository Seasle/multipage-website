{
  "name": "multipage-website",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently npm:dev:*",
    "dev:gulp": "npx gulp dev",
    "dev:eleventy": "npx @11ty/eleventy --config=eleventy.config.сjs --serve",
    "build": "npm run clear && concurrently npm:build:*",
    "build:gulp": "npx gulp build",
    "build:eleventy": "npx @11ty/eleventy --config=eleventy.config.сjs",
    "clear": "del-cli dist -f"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.0.8"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "del-cli": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-esbuild": "^0.12.0",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "postcss-csso": "^6.0.1",
    "postcss-import": "^15.1.0",
    "sass": "^1.69.5"
  }
}
